{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "back-end.src.main:app",
                "--reload",
                "--port",
                "8000",
                "--env-file",
                "back-end/.env"
            ],
            "jinja": false,
            "env": {"PYTHONPATH": "${env:PYTHONPATH}"}
        }
    ]
}
